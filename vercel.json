{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "(.*)",
            "dest": "src/main.ts"
        }
    ],
    "env": {
        "NODE_ENV": "development",
        "dependencies": {
            "mysql2": "^3.6.1"
        }
    }
}